<div class="get-in-touch text-center">
  <div class="container">
    <div class="row clearfix">
      <div class="col-xs-12 col-md-12 col-lg-12">
        <p>Email : <span class="email">katiegrog@gmail.com</span></p>
      </div>
    </div>
  </div>
</div>

<section class="section section--alternative u-relative">
  <div class="container">
    <div class="row clearfix">
      <div class="text-center">
        <h3 class="text-secondary u-margin-b-100 u-padding-x-100">You scrolled all the way down here...</h3>

        <h2 class="u-margin-b-100 text-uppercase display-text"><span class="text-primary text-underline">H</span><span class="text-secondary text-underline">o</span><span class="text-tertiary text-underline">o</span><span class="text-primary text-underline">r</span><span class="text-secondary text-underline">a</span><span class="text-tertiary text-underline">y</span><span class="text-primary text-underline">!</span></h2>
      </div>
    </div>

    <div class="row">
      <div class="text-center">
        <p class="text-tertiary party-mode-link js-party-mode u-padding-y-50">Click here for party mode!</p>
      </div>
    </div>

    <div class="row">
      <div class="text-center">
        <p class="text-tertiary party-mode-off-link js-party-mode-off">Eugh, turn that off</p>
      </div>
    </div>

    <div class="u-margin-t-100">
      <div class="row clearfix">
        <div class="col-xs-12 col-sm-4 col-md-4 col-lg-4">
          <div class="u-margin-b-100">
            <div class="square square-1"></div>
            <div class="square square-1--absolute">
              <%= image_tag 'face.svg', class: 'image-face' %>
            </div>
          </div>
        </div>

        <div class="col-xs-12 col-sm-4 col-md-4 col-lg-4">
          <div class="u-margin-b-100">
            <div class="square square-2"></div>
            <div class="square square-2--absolute">
              <%= image_tag 'face.svg', class: 'image-face' %>
            </div>
          </div>
        </div>

        <div class="col-xs-12 col-sm-4 col-md-4 col-lg-4">
          <div class="square square-3"></div>
          <div class="square square-3--absolute">
            <%= image_tag 'face.svg', class: 'image-face' %>
          </div>
        </div>
      </div>

      <%= image_tag 'triangle.svg', class: 'absolute-shapes circle-absolute-1' %>
      <%= image_tag 'triangle.svg', class: 'absolute-shapes circle-absolute-2'%>
      <%= image_tag 'triangle.svg', class: 'absolute-shapes circle-absolute-3'%>
      <%= image_tag 'triangle-pink.svg', class: 'absolute-shapes triangle-absolute-1' %>
      <%= image_tag 'triangle.svg', class: 'absolute-shapes triangle-absolute-2' %>
      <%= image_tag 'triangle-pink.svg', class: 'absolute-shapes triangle-absolute-3' %>
      <%= image_tag 'triangle-pink.svg', class: 'absolute-shapes triangle-absolute-4' %>
      <%= image_tag 'triangle.svg', class: 'absolute-shapes triangle-absolute-5' %>
      <%= image_tag 'triangle-pink.svg', class: 'absolute-shapes triangle-absolute-6' %>
    </div>
  </div>
</section>

<%= render 'shared/footer' %>

</html>
